<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :type %>
    <%= f.input :rules %>
    <%= f.input :judging %>
    <%= f.input :event_date_time %>
    <%= f.input :team_size %>
    <%= f.input :venue %>
    <%= f.input :eligibilty %>
    <%= f.input :registration_deadline %>
    <%= f.input :prizes %>
    <%= f.input :contact %>
    <%= f.input :photo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
