<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Rules</th>
      <th>Judging</th>
      <th>Event date time</th>
      <th>Team size</th>
      <th>Venue</th>
      <th>Eligibilty</th>
      <th>Registration deadline</th>
      <th>Prizes</th>
      <th>Contact</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.type %></td>
        <td><%= event.rules %></td>
        <td><%= event.judging %></td>
        <td><%= event.event_date_time %></td>
        <td><%= event.team_size %></td>
        <td><%= event.venue %></td>
        <td><%= event.eligibilty %></td>
        <td><%= event.registration_deadline %></td>
        <td><%= event.prizes %></td>
        <td><%= event.contact %></td>
        <td><%= event.photo %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
