<h1>Listing participant_teams</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Participant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @participant_teams.each do |participant_team| %>
      <tr>
        <td><%= participant_team.team %></td>
        <td><%= participant_team.participant %></td>
        <td><%= link_to 'Show', participant_team %></td>
        <td><%= link_to 'Edit', edit_participant_team_path(participant_team) %></td>
        <td><%= link_to 'Destroy', participant_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Participant team', new_participant_team_path %>
