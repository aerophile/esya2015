<h1>Listing event_admins</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_admins.each do |event_admin| %>
      <tr>
        <td><%= event_admin.event %></td>
        <td><%= event_admin.user %></td>
        <td><%= link_to 'Show', event_admin %></td>
        <td><%= link_to 'Edit', edit_event_admin_path(event_admin) %></td>
        <td><%= link_to 'Destroy', event_admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event admin', new_event_admin_path %>
